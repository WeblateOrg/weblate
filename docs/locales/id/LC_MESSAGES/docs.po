msgid "`regex documentation <https://github.com/mrabarnett/mrab-regex>`_"
msgstr "`dokumentasi regex <https://github.com/mrabarnett/mrab-regex>`_"

#: ../../user/checks.rst:1609
msgid "reStructuredText syntax error"
msgstr "Kesalahan sintaks reStructuredText"

#: ../../user/checks.rst:1613
msgid "reStructuredText syntax error in the translation."
msgstr "Kesalahan sintaks reStructuredText dalam terjemahan."

#: ../../user/checks.rst:1621
