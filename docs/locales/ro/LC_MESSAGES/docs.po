msgstr ""
"gettext MO (disponibil numai atunci când traducerea utilizează gettext PO)"

#: ../../user/files.rst:36
msgid "CSV"
msgstr "CSV"

#: ../../user/files.rst:38
msgid "JSON (only available for monolingual translations)"
msgstr "JSON (disponibil numai pentru traducerile monolingve)"

