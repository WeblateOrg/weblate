#!/bin/sh
# Fake key scan program to return SSH key
if [ "$1" = "1.2.3.4" ]; then
    exit 1
elif [ "$1" = "2001:0db8:85a3:0000:0000:8a2e:0370:7334" ]; then
    echo test error >&2
    exit 2
fi
echo '# github.com SSH-2.0-libssh-0.7.0'
echo 'github.com ssh-rsa '
echo '# github.com SSH-2.0-libssh-0.7.0'
echo 'no hostkey alg'
