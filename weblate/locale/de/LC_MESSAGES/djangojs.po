#, javascript-format
msgid "Ctrl+M then %s"
msgstr "Strg+M, dann %s"

#: weblate/static/editor/full.js:450
#, javascript-format
msgid "Press Cmd+I then %s to dismiss this."
msgstr "Drücken Sie Befehlstaste+I, dann %s, um dies zu verwerfen."

#: weblate/static/editor/full.js:455
#, javascript-format
msgid "Press Ctrl+I then %s to dismiss this."
msgstr "Drücken Sie Strg+I, dann %s, um dies zu verwerfen."

