#, javascript-format
msgid "Ctrl+M then %s"
msgstr "Ctrl+M puis %s"

#: weblate/static/editor/full.js:450
#, javascript-format
msgid "Press Cmd+I then %s to dismiss this."
msgstr "Appuyez sur Cmd&nbsp;+&nbsp;I puis %s pour ignorer."

#: weblate/static/editor/full.js:455
#, javascript-format
msgid "Press Ctrl+I then %s to dismiss this."
msgstr "Appuyez sur Ctrl + I puis %s pour ignorer."

