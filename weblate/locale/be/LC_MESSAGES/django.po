msgstr "Кампанент: {}"

#: weblate/accounts/models.py:79
msgid "Password changed."
msgstr "Пароль зменены."

#: weblate/accounts/models.py:80
#, python-brace-format
msgid "Username changed from {old} to {new}."
msgstr "Імя карыстальніка было зменена з {old} на {new}."

#: weblate/accounts/models.py:81
#, python-brace-format
msgid "E-mail changed from {old} to {new}."
msgstr "Адрас электроннай пошты зменены з {old} на {new}."

#: weblate/accounts/models.py:82
#, python-brace-format
msgid "Full name changed from {old} to {new}."
msgstr "Поўная назва змененая з {old} на {new}."

#: weblate/accounts/models.py:83
msgid "Password reset requested."
msgstr "Патрабуецца скінуць пароль."

#: weblate/accounts/models.py:84
msgid "Password reset confirmed, password turned off."
msgstr "Скід пароля пацверджаны і пароль адключаны."

#: weblate/accounts/models.py:85
#, python-brace-format
msgid "You can now log in using {method} ({name})."
msgstr "Зараз вы можаце ўвайсці выкарыстоўваючы {method} ({name})."

#: weblate/accounts/models.py:86
#, python-brace-format
msgid "You can no longer log in using {method} ({name})."
msgstr "Вы больш не можаце ўвайсці выкарыстоўваючы {method} ({name})."

#: weblate/accounts/models.py:87
#, python-brace-format
msgid "Logged on using {method} ({name})."
msgstr "Увайшоў выкарыстоўваючы {method} ({name})."

#: weblate/accounts/models.py:88
#, python-brace-format
msgid "Logged on using {method} ({name}) from a new device."
msgstr "Увайшоў выкарыстоўваючы {method} ({name}) з новай прылады."

#: weblate/accounts/models.py:89
msgid "Somebody has attempted to register with your e-mail."
msgstr "Нехта спрабаваў зарэгістравацца з вашым адрасам электроннай пошты."

#: weblate/accounts/models.py:90
msgid "Somebody has attempted to register using your e-mail address."
msgstr "Нехта спрабаваў зарэгістравацца з вашым адрасам электроннай пошты."

#: weblate/accounts/models.py:91
#, python-brace-format
msgid "Could not log in using {method} ({name})."
msgstr "Не ўдалося ўвайсці выкарыстоўваючы {method} ({name})."

#: weblate/accounts/models.py:92
msgid "Account locked due to many failed logins."
msgstr "Акаўнт заблакаваны праз празмерную колькасць няўдалых уваходаў."

