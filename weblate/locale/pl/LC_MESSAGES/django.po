msgid "Commit message when translating"
msgstr "Opis commita przy tłumaczeniu"

#: weblate/trans/models/component.py:628 weblate/trans/models/component.py:637
#: weblate/trans/models/component.py:646 weblate/trans/models/component.py:655
#: weblate/trans/models/component.py:664 weblate/trans/models/component.py:673
