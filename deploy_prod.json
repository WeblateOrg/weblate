{
  "steps": [
    {
      "name": "gcr.io/cloud-builders/kubectl",
      "args": [
        "set",
        "image",
        "gcr.io/repcore-prod/weblate:$COMMIT_SHA",
        "--namespace",
        "weblate-prod"
      ],
      "env": {
        "CLOUDSDK_CONTAINER_CLUSTER": "vendasta-central-regional",
        "CLOUDSDK_COMPUTE_REGION": "us-central1"
      }
    }
  ]
}
